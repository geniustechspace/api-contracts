{
  "name": "@geniustechspace/api-contracts-clients",
  "version": "0.1.0",
  "description": "TypeScript clients for GeniusTechSpace API contracts",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "build": "pnpm run -r build",
    "clean": "pnpm run -r clean && rm -rf node_modules",
    "lint": "pnpm run -r lint",
    "test": "pnpm run -r test",
    "generate": "buf generate",
    "typecheck": "pnpm run -r typecheck"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "typescript": "^5.3.0",
    "prettier": "^3.1.0",
    "eslint": "^8.55.0",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@typescript-eslint/parser": "^6.14.0"
  },
  "engines": {
    "node": ">=16",
    "pnpm": ">=8"
  },
  "packageManager": "pnpm@8.12.0"
}
